{
  "functions": {
    "api/**/*.php": { "runtime": "vercel-php@0.6.0" }
  },
  "routes": [
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/config/(.*)", "dest": "/config/$1" },
    { "src": "/classes/(.*)", "dest": "/classes/$1" },
    { "src": "/(.*)", "dest": "/api/index.php" }
  ]
}
